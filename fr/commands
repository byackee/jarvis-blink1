*etei*la*led*== say "led eteinte" && say "$(sudo ./plugins/jarvis-blink1/resources/blink1-tool-armv7l --off)"
*allume led*== say "led allumée" && say "$(sudo ./plugins/jarvis-blink1/resources/blink1-tool-armv7l --on)"
*regle*led*couleur*bleu*== say "led reglée sur la couleur bleue" && say "$(sudo ./plugins/jarvis-blink1/resources/blink1-tool-armv7l --blue)"
*regle*led*couleur*verte*== say "led reglée sur la couleur verte" && say "$(sudo ./plugins/jarvis-blink1/resources/blink1-tool-armv7l --green)"
*regle*led*couleur*blanc*== say "led reglée sur la couleur blanche" && say "$(sudo ./plugins/jarvis-blink1/resources/blink1-tool-armv7l --white)"
*regle*led*couleur*jaune*== say "led reglée sur la couleur jaune" && say "$(sudo ./plugins/jarvis-blink1/resources/blink1-tool-armv7l --yellow)"
*regle*led*couleur*orange*== say "led reglée sur la couleur orange" && say "$(sudo ./plugins/jarvis-blink1/resources/blink1-tool-armv7l --orange)"
*regle*led*couleur*cyan*== say "led reglée sur la couleur cyan" && say "$(psudo ./plugins/jarvis-blink1/resources/blink1-tool-armv7l --cyan)"
